<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Archetype Created Web Application</display-name>
  <servlet>
  	<servlet-name>LoginRegisterController</servlet-name>
  	<display-name>LoginRegisterController</display-name>
  	<description></description>
  	<servlet-class>org.bildit.controller.LoginRegisterController</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>RegistrationFormController</servlet-name>
  	<display-name>RegistrationFormController</display-name>
  	<description></description>
  	<servlet-class>org.bildit.controller.RegistrationFormController</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>LoginFormController</servlet-name>
  	<display-name>LoginFormController</display-name>
  	<description></description>
  	<servlet-class>org.bildit.controller.LoginFormController</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>EditProfileController</servlet-name>
  	<display-name>EditProfileController</display-name>
  	<description></description>
  	<servlet-class>org.bildit.controller.EditProfileController</servlet-class>
  </servlet>
  <servlet-mapping>
  	<servlet-name>LoginRegisterController</servlet-name>
  	<url-pattern>/loginRegister</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>RegistrationFormController</servlet-name>
  	<url-pattern>/registration</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>LoginFormController</servlet-name>
  	<url-pattern>/login</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>EditProfileController</servlet-name>
  	<url-pattern>/editProfile</url-pattern>
  </servlet-mapping>
</web-app>
